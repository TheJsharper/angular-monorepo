{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@angular-monorepo/primeng-data-view": [
        "libs/primeng-data-view/src/index.ts"
      ],
      "@angular-monorepo/primeng-org-chart": [
        "libs/primeng-org-chart/src/index.ts"
      ],
      "@angular-monorepo/primeng-pick-list": [
        "libs/primeng-pick-list/src/index.ts"
      ],
      "@angular-monorepo/primeng-table": ["libs/primeng-table/src/index.ts"],
      "@angular-monorepo/primeng-timeline": [
        "libs/primeng-timeline/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
